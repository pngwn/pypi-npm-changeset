{
  "name": "pypi-npm-changeset",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ci:version": "changeset version && pnpm i --lockfile-only && node ./fix_changelogs.cjs",
    "ci:publish": "pnpm publish --no-git-checks --access public -r --filter=@pngwn/*",
    "refresh": "cp -r ./_changesets/*.md ./.changeset/",
    "build-action": "esbuild .github/actions/publish-pypi/index.ts --bundle --platform=node --outfile=.github/actions/publish-pypi/dist/index.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@changesets/cli": "^2.26.2",
    "@changesets/get-github-info": "^0.5.2",
    "@manypkg/get-packages": "^2.2.0",
    "esbuild": "^0.18.11"
  },
  "type": "module",
  "private": true
}
